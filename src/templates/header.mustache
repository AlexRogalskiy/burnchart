      {{user.displayName}} logged in
    {{else}}
      <a class="github" on-click="!login"><Icons icon="github"/> Sign In</a>
    {{/user.displayName}}
  </div>

  <a id="icon" href="#">
    <Icons icon="{{icon}}"/>
  </a>

  <div class="q">
    <Icons icon="search"/>
    <Icons icon="down-open"/>
